{
  "name": "@mcpjam/inspector",
  "productName": "MCPJam Inspector",
  "version": "1.5.5",
  "type": "module",
  "description": "MCPJam Inspector",
  "license": "Apache-2.0",
  "author": "MCPJam (https://mcpjam.com)",
  "homepage": "https://mcpjam.com",
  "bugs": "https://github.com/mcpjam/inspector/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/MCPJam/inspector.git"
  },
  "main": ".vite/build/main.cjs",
  "bin": {
    "inspector": "bin/start.js",
    "inspector-vite": "bin/start.js"
  },
  "exports": {
    ".": {
      "types": "./dist/lib/launcher/index.d.ts",
      "import": "./dist/lib/launcher/index.js",
      "require": "./dist/lib/launcher/index.cjs"
    },
    "./vite": {
      "types": "./dist/lib/vite/index.d.ts",
      "import": "./dist/lib/vite/index.js",
      "require": "./dist/lib/vite/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "bin",
    "dist",
    "assets/icon.ico",
    "assets/icon.icns",
    ".env.production",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:convex": "convex dev",
    "dev:server": "cross-env ENVIRONMENT=dev tsx watch --include 'evals-cli/src/**/*.ts' --exclude 'evals-cli/dist/**' --exclude 'evals-cli/node_modules/**' server/index.ts",
    "dev:client": "vite --config client/vite.config.ts",
    "dev:client:clean": "rm -rf node_modules/.vite && FORCE_OPTIMIZE=true vite --config client/vite.config.ts",
    "build": "npm run clean && npm run build:sdk && npm run build:lib && npm run build:client && npm run build:server",
    "build:sdk": "npm --prefix ../sdk run build",
    "bundle:chatgpt": "node scripts/bundle-openai-script.js",
    "clean": "rm -rf dist out .vite client/dist",
    "clean:all": "rm -rf dist out .vite client/dist node_modules sdk/node_modules",
    "deps:ci": "npm ci --legacy-peer-deps --cache .npm-cache && npm --prefix ../sdk ci --legacy-peer-deps --cache .npm-cache",
    "build:lib": "tsup --config lib/tsup.config.ts",
    "build:client": "vite build --config client/vite.config.ts",
    "build:client:clean": "rm -rf node_modules/.vite && FORCE_OPTIMIZE=true vite build --config client/vite.config.ts",
    "build:server": "tsup --config server/tsup.config.ts",
    "start": "cross-env NODE_ENV=production node bin/start.js",
    "prettier-fix": "prettier --write .",
    "install:deps": "npm install --legacy-peer-deps && npm --prefix ../sdk install --legacy-peer-deps",
    "docker:build": "docker build -t mcpjam/mcp-inspector:local -f Dockerfile ..",
    "docker:start": "docker run -d -p 127.0.0.1:6274:6274 --name mcp-inspector --pull=never mcpjam/mcp-inspector:local",
    "docker:stop": "docker stop mcp-inspector && docker rm mcp-inspector || true",
    "docker:dev": "npm run docker:stop; npm run docker:build && npm run docker:start",
    "electron:start": "electron-forge start",
    "electron:package": "electron-forge package",
    "icons:win": "node scripts/generate-windows-icon.mjs",
    "icons:mac": "node scripts/generate-mac-icon.mjs",
    "electron:make": "npm run icons:win && npm run icons:mac && electron-forge make",
    "electron:install": "killall 'MCPJam Inspector' 2>/dev/null || true && rm -rf '/Applications/MCPJam Inspector.app' && cp -r 'out/MCPJam Inspector-darwin-arm64/MCPJam Inspector.app' /Applications/ && open '/Applications/MCPJam Inspector.app'",
    "electron:publish": "electron-forge publish",
    "electron:dev": "electron-forge start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/azure": "^2.0.79",
    "@ai-sdk/deepseek": "^1.0.5",
    "@ai-sdk/google": "^2.0.11",
    "@ai-sdk/mistral": "^2.0.19",
    "@ai-sdk/openai": "^2.0.32",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/react": "^2.0.79",
    "@ai-sdk/xai": "^2.0.29",
    "@convex-dev/auth": "^0.0.88",
    "@convex-dev/workos": "^0.0.1",
    "@hono/node-server": "^1.13.7",
    "@hookform/resolvers": "^3.10.0",
    "@mcp-ui/client": "^5.9.0",
    "@mcpjam/sdk": "file:../sdk",
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@ngrok/ngrok": "^1.6.0",
    "@openrouter/ai-sdk-provider": "^2.0.2",
    "@sentry/electron": "^5.10.0",
    "@sentry/node": "^8.47.0",
    "@sentry/react": "^8.47.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@workos-inc/authkit-react": "^0.12.0",
    "@xyflow/react": "^12.9.0",
    "ai": "^6.0.50",
    "ajv": "^8.17.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.26.2",
    "date-fns": "^3.6.0",
    "denque": "^2.1.0",
    "dotenv": "^16.4.5",
    "electron-log": "^5.4.2",
    "embla-carousel-react": "^8.6.0",
    "fast-deep-equal": "^3.1.3",
    "fix-path": "^4.0.0",
    "framer-motion": "^12.23.6",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "hono": "^4.11.4",
    "lucide-react": "^0.525.0",
    "marked": "^16.4.1",
    "next-themes": "^0.4.6",
    "ollama-ai-provider-v2": "^1.5.2",
    "open": "^10.2.0",
    "postcss": "^8.5.6",
    "posthog-js": "^1.310.0",
    "posthog-node": "^5.9.2",
    "radix-ui": "^1.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.3",
    "react-textarea-autosize": "^8.5.9",
    "react18-json-view": "^0.2.9",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "simple-icons": "^15.6.0",
    "sonner": "^2.0.6",
    "streamdown": "^1.4.0",
    "tailwind-merge": "^3.3.1",
    "tokenlens": "^1.3.1",
    "update-electron-app": "^3.1.1",
    "url-template": "^3.1.1",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.24.6",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.2",
    "@electron-forge/maker-deb": "^7.8.2",
    "@electron-forge/maker-dmg": "^7.8.2",
    "@electron-forge/maker-rpm": "^7.8.2",
    "@electron-forge/maker-squirrel": "^7.8.2",
    "@electron-forge/maker-zip": "^7.8.2",
    "@electron-forge/plugin-fuses": "^7.8.2",
    "@electron-forge/plugin-vite": "^7.8.2",
    "@electron-forge/publisher-github": "^7.8.2",
    "@electron/fuses": "^1.8.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "cross-env": "^10.1.0",
    "electron": "^37.2.5",
    "eslint": "^9",
    "jsdom": "^27.4.0",
    "png-to-ico": "^2.1.8",
    "sharp": "^0.33.4",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vite": "^7.1.4",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "tmp": "0.2.5",
    "jsondiffpatch": "0.7.2",
    "zod": "^4.1.12",
    "qs": "6.14.1"
  }
}
